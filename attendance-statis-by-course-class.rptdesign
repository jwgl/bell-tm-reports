            <xml-property name="queryText"><![CDATA[with course_class as (
  select id
  from ea.course_class
  where id = cast(? as uuid)
), course_class_student as (
  select distinct student.id as id,
    student.name as name,
    subject.name as subject,
    admin_class.name as adminClass
  from course_class
  join ea.task on course_class.id = task.course_class_id
  join ea.task_student on task.id = task_student.task_id
  join ea.student on task_student.student_id = student.id
  join ea.major on student.major_id = major.id
  join ea.subject on major.subject_id = subject.id
  join ea.admin_class on student.admin_class_id = admin_class.id
), attendance_stats as (
  select sa.student_id as student_id,
    sum(case when type = 1 then ts.total_section else 0 end) as absent,
    sum(case when type = 2 then 0.5 else 0 end) as late,
    sum(case when type in (3, 5) then ts.total_section else 0 end) as early,
    sum(case when type in (1, 3, 5) then ts.total_section else 0 end) + sum(case type when 2 then 0.5 else 0 end) as total,
    sum(case type when 4 then ts.total_section else 0 end) as leave
  from tm.dv_student_attendance sa
  join ea.task_schedule ts on sa.task_schedule_id = ts.id
  join ea.task on ts.task_id = task.id
  join course_class on task.course_class_id = course_class.id 
  where sa.valid = true
  group by sa.student_id
)
select id, name, subject, adminClass,
  nullif(absent, 0)::numeric(4,1) as absent,
  nullif(late, 0)::numeric(4,1) as late,
  nullif(early, 0)::numeric(4,1) as early,
  nullif(total, 0)::numeric(4,1) as total,
  nullif(leave, 0)::numeric(4,1) as leave
from course_class_student
left join attendance_stats on id = student_id
order by id;
]]></xml-property>
