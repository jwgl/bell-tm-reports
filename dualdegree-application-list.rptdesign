            <xml-property name="queryText"><![CDATA[select st.name as studentName,
	st.id as studentId,
	af.university_cooperative,
	af.major_cooperative,
	af.linkman,
	af.phone,
	af.email,
	af.date_submitted,
	af.status,
	dt.name as departmentName
from tm_dual.degree_application af 
join tm_dual.paper_form pf on af.id=pf.form_id
join ea.student st on st.id=af.student_id
join ea.department dt on st.department_id=dt.id
where af.award_id=? and dt.id=?;]]></xml-property>
